import random
import datetime


employees = 200
projects = 9


def random_date(start, end):
    """
    """

    delta = end - start
    int_delta = (delta.days * 24 * 60 * 60)
    random_data = random.randrange(int_delta)
    return start + random_data

d1 = datetime.strptime('2012-1-1', '%Y-%m-%d')
d2 = datetime.date.today()

print(random_data(d1,d2))
